GATEWAY_PORT=8000

REDIS_URL=redis://redis:6379/0

POSTGRES_DB=campus
POSTGRES_USER=campus
POSTGRES_PASSWORD=campus
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

NATS_URL=nats://nats:4222

QDRANT_URL=http://qdrant:6333

SINGLE_MODE=0

# Lead-Agent System Configuration

# LLM Configuration
LLM_PROVIDER=openai
LLM_MODEL=gpt-4

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Database
DB_CONNECTION=mysql
SECRET_KEY=your_secret_key_here

# Lead-Agent Specific Settings
LEAD_AGENT_MAX_WORKFLOWS=100
LEAD_AGENT_WORKFLOW_TTL=86400
LEAD_AGENT_COMPLEXITY_THRESHOLD=0.7
LEAD_AGENT_ENABLE_CRITIC=true

# Action Executor Settings
ACTION_EXECUTOR_TIMEOUT=30
ACTION_EXECUTOR_RETRY_ATTEMPTS=3

# Enhanced RAG Settings
RAG_SEARCH_THRESHOLD=0.7
RAG_MAX_CITATIONS=5
RAG_ENABLE_RERANKING=true

# Critic Agent Settings
CRITIC_EVALUATION_ENABLED=true
CRITIC_MIN_SCORE_THRESHOLD=7.0

# Performance Settings
MAX_CONCURRENT_WORKFLOWS=10
AGENT_RESPONSE_TIMEOUT=60
MEMORY_CLEANUP_INTERVAL=3600
